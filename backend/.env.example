# Optional: OpenRouteService API key (free tier — 2000 requests/day)
# Sign up at https://openrouteservice.org/dev/#/signup
ORS_API_KEY=

# OSRM server URL (public demo — for production, self-host)
OSRM_BASE_URL=https://router.project-osrm.org

# Nominatim server URL (public — for production, self-host)
NOMINATIM_BASE_URL=https://nominatim.openstreetmap.org

# Overpass API URL
OVERPASS_BASE_URL=https://overpass-api.de/api/interpreter

# Optional: Google Maps Routes API key (for real transit routing)
# Enable "Routes API" at https://console.cloud.google.com/apis
# $200/month free credit ≈ 40k transit requests
GOOGLE_MAPS_API_KEY=

# Flask
FLASK_ENV=development
FLASK_DEBUG=1
